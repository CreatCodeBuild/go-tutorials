type Query {
    questions(
        search: String
    ): [Question!]!
}

interface Post {
    id: Int!
    content: String!
}

type Question implements Post {
    id: Int!
    title: String!
    content: String!
    # comments: [Comment]
    # answers: [Answer]
}

